// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
    id       String @id @default(auto()) @map("_id") @db.ObjectId
    username String?
    budget Float?
    rent Float?
    utilities Float?
    food Float?
    living Float?
    misc Float?
    income Float?
}